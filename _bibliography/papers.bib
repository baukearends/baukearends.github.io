	journal = {European Journal of Clinical Investigation},
	author = {Arends, Bauke K. O. and McCormick, Jenna M. and Van Der Harst, Pim and Heus, Pauline and Van Es, René},
	month = apr,
	year = {2025},
	pages = {e14387},
	file = {Arends et al. - 2025 - Barriers, facilitators and strategies for the impl.pdf:/Users/barends4/Zotero/storage/LTV7KVCN/Arends et al. - 2025 - Barriers, facilitators and strategies for the impl.pdf:application/pdf},
