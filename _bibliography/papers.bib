	number = {1},
	urldate = {2025-06-13},
	journal = {BMC Medical Informatics and Decision Making},
	author = {Arends, Bauke and Vessies, Melle and Van Osch, Dirk and Teske, Arco and Van Der Harst, Pim and Van Es, René and Van Es, Bram},
